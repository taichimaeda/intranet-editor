{
  "name": "Intranet Editor",
  "version": "1.0",
  "description": "Edit Intranet as you like",
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "contextMenus",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Popup",
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://sp.rosey.ch/AgendaEleve*"
      ],
      "js": [
        "js/content.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2
}